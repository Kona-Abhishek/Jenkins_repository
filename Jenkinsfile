pipeline {
    agent any
stages {
        stage('build') {
            steps {
                echo 'Build'
                git 'https://github.com/Kona-Abhishek/Jenkins_repository.git'
            }
        }
        stage('deploy') {
            steps {
                echo 'Deploy'
                git 'https://github.com/Kona-Abhishek/Jenkins_repository.git'
            }
        }
        stage('release') {
            steps {
                echo 'Release'
                git 'https://github.com/Kona-Abhishek/Jenkins_repository.git'
            }
        }
        stage('test') {
            steps {
                echo 'Test'
                git 'https://github.com/Kona-Abhishek/Jenkins_repository.git'
            }
        }
    }
}
    
